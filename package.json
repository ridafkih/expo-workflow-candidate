{
  "name": "maxrewards",
  "version": "1.3.14",
  "description": "The MaxRewards application is the application where your credit cards live.",
  "main": "index.js",
  "repository": "https://github.com/maxrewards/maxrewards",
  "author": "Rida F'kih <hello@rida.dev>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "postinstall": "dephash hash --output .hash --eas-only",
    "build:development": "eas build --profile development",
    "build:simulator": "eas build --profile simulator",
    "hash": "dephash hash",
    "hash:raw": "dephash hash -r",
    "start": "expo start --dev-client",
    "lint": "eslint --ext js,ts,tsx .",
    "doctor": "npx expo-doctor",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@babel/preset-env": "^7.1.6",
    "@expo/vector-icons": "13.0.0",
    "expo": "48.0.11",
    "expo-constants": "14.2.1",
    "expo-dev-client": "2.2.1",
    "expo-font": "11.1.1",
    "expo-image": "1.0.0",
    "expo-linear-gradient": "12.1.2",
    "expo-linking": "4.0.1",
    "expo-router": "1.5.3",
    "expo-splash-screen": "0.18.1",
    "expo-status-bar": "1.4.4",
    "expo-updates": "0.16.4",
    "metro": "~0.76.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.6",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "3.20.0",
    "react-native-svg": "^13.9.0",
    "react-native-svg-transformer": "1.0.0",
    "react-native-web": "0.18.10"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@expo/config-types": "48.0.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-native": "0.71.6",
    "babel-preset-expo": "9.3.0",
    "dephash": "4.0.0",
    "eslint": "8.35.0",
    "eslint-config-universe": "11.2.0",
    "eslint-plugin-react": "7.32.2",
    "prettier": "2.8.4",
    "typescript": "4.9.4"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      }
    },
    "plugins": [
      "react"
    ],
    "ignorePatterns": [
      "node_modules",
      "build",
      ".expo",
      ".expo-shared"
    ],
    "overrides": [
      {
        "files": [
          "*.js"
        ],
        "env": {
          "node": true
        }
      }
    ]
  }
}
